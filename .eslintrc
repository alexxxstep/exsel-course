{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "space-before-function-paren": "off",
    "object-curly-spacing": "off",
    "indent": [4, "tab"],
    "no-tabs": 0
  },
  "extends": ["eslint:recommended", "google"]
}
